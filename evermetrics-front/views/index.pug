  body
    section#header-container
    section#main-container
    footer.site-footer
      .container
        .row(style='padding-top:20px;')
          .col.s12.l4(style='text-align: center')
            img(style='margin-top: 100px', src='logo-eversocial-blanco.png')
          .col.s12.l5
            h6  OTROS MESES
            .col.s12.l5
              a(href='#')
